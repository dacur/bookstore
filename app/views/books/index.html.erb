<h1>Bookstore</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Author</th>
      <th>Published Date</th>
      <th>Price</th>
    </tr>
  <% @books.each do |book| %>
    <tr>
      <td><%= book.title %></td>
      <td><%= book.author %></td>
      <td><%= book.published_date %></td>
      <td><%= book.price_cents %></td>
    </tr>
    <% end %>
</table>